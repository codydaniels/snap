<body class="flex justify-center dark:bg-black" x-data={'darkMode': true}>

  {# Check current browser size with a media query, for Javascript #}
  {# <div id="getSize"></div> #}

  <div class="w-full max-w-ult">
    {% include "header.njk" %}
    {{ content | safe }}
     {% include "footer.njk" %}
  </div> 

  <script src="/all.js" async defer></script>
  {{ '<script>
    if (window.netlifyIdentity) {
      window.netlifyIdentity.on("init", user => {
        if (!user) {
          window.netlifyIdentity.on("login", () => {
            document.location.href = "/admin/";
          });
        }
      });
    }
  </script>' if '/admin' in page.url }}

</body>

