  {# Load tracking codes #}
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {# Social media metadata #}
  <meta property="og:url" content="https://TODO.com{{ page.url }}">
  <meta property="og:site_name" content="TODO">
  <meta property="og:title" content="{{ title }}">
  <meta property="og:description" content="{{ description }}">
  <meta property="og:type" content="article" />
  <meta property="og:image" content="https://TODO.com{{ image }}">
  <meta name="twitter:image:alt" content="{{ image_alt }}">
  <meta name="twitter:card" content="summary_large_image">
  {# Browser tab #}
  <title>{{ title }}</title>
  <link rel="icon" type="image/x-icon" href="/assets/favicon.png">
  {# CSS #}
  <style>
    /*Prevent unstyled content flash*/
    html{ visibility:hidden; opacity:0; }
    /*Load fonts without chained requests*/
    @font-face {
      font-family: 'Rubik';
      src: url('/assets/fonts/rubik.ttf');
      font-style: normal;
      font-stretch: normal;
    }
    @font-face {
      font-family: 'Kallisto Bold';
      src: url('/assets/fonts/kallisto-bold.woff');
      font-style: normal;
      font-stretch: normal;
    }
  </style>
